<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#DAD0ED"
    tools:context=".UI.Ui.LK">

    <RelativeLayout
        android:id="@+id/relativeLayout5"
        android:layout_width="405dp"
        android:layout_height="387dp"
        android:background="#DAD0ED"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.461"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.686">

        <Button
            android:id="@+id/button23"
            android:layout_width="187dp"
            android:layout_height="53dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="120dp"
            android:layout_marginTop="197dp"
            android:layout_marginEnd="107dp"
            android:layout_marginBottom="137dp"
            android:background="@drawable/buttonst"
            android:fontFamily="serif-monospace"
            android:gravity="center"
            android:onClick="Login_Button"
            android:rotation="0"
            android:rotationX="0"
            android:rotationY="0"
            android:text="Сменить пользователя"
            android:textAllCaps="false"
            android:textSize="18sp" />

        <Button
            android:id="@+id/button9"
            android:layout_width="wrap_content"
            android:layout_height="57dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="120dp"
            android:layout_marginTop="11dp"
            android:layout_marginEnd="114dp"
            android:layout_marginBottom="320dp"
            android:background="@drawable/buttonst"
            android:fontFamily="serif-monospace"
            android:gravity="center"
            android:onClick="Tovary_Btn"
            android:rotation="0"
            android:rotationX="0"
            android:rotationY="0"
            android:text="Товары"
            android:textAllCaps="false"
            android:textSize="18sp" />

        <Button
            android:id="@+id/button12"
            android:layout_width="168dp"
            android:layout_height="53dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="120dp"
            android:layout_marginTop="75dp"
            android:layout_marginEnd="114dp"
            android:layout_marginBottom="259dp"
            android:background="@drawable/buttonst"
            android:fontFamily="serif-monospace"
            android:gravity="center"
            android:rotation="0"
            android:rotationX="0"
            android:rotationY="0"
            android:text="Пункты выдачи"
            android:textAllCaps="false"
            android:textSize="18sp" />

        <SeekBar
            android:id="@+id/seekBar"
            android:layout_width="174dp"
            android:layout_height="69dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="298dp"
            android:layout_marginTop="59dp"
            android:layout_marginEnd="-66dp"
            android:layout_marginBottom="259dp"
            android:rotation="90"
            android:rotationX="0"
            android:rotationY="0" />

        <TextView
            android:id="@+id/textView21"
            android:layout_width="405dp"
            android:layout_height="18dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="2dp"
            android:layout_marginTop="263dp"
            android:layout_marginEnd="-2dp"
            android:layout_marginBottom="106dp"
            android:text="__________________________________________________________________________________________________________________________________________________"
            android:textSize="16sp" />

        <Switch
            android:id="@+id/switch2"
            android:layout_width="wrap_content"
            android:layout_height="51dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="-47dp"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="346dp"
            android:layout_marginBottom="332dp"
            android:fontFamily="serif-monospace"
            android:rotation="0"
            android:rotationX="0"
            android:rotationY="0"
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="15dp"
            android:layout_marginTop="48dp"
            android:layout_marginEnd="358dp"
            android:layout_marginBottom="307dp"
            app:srcCompat="@android:drawable/ic_dialog_email" />

        <EditText
            android:id="@+id/editTextTextPostalAddress"
            android:layout_width="387dp"
            android:layout_height="52dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="10dp"
            android:layout_marginTop="330dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="4dp"
            android:ems="10"
            android:gravity="center"
            android:inputType="textPostalAddress"
            android:text="г. Москва. Проспект Вернадского, 78" />

        <TextView
            android:id="@+id/textView22"
            android:layout_width="wrap_content"
            android:layout_height="29dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="100dp"
            android:layout_marginTop="294dp"
            android:layout_marginEnd="100dp"
            android:layout_marginBottom="64dp"
            android:background="@drawable/buttonst"
            android:fontFamily="serif-monospace"
            android:gravity="center"
            android:text="Введите ваш адрес"
            android:textColor="#353535"
            android:textSize="18sp" />

        <Button
            android:id="@+id/button3"
            android:layout_width="168dp"
            android:layout_height="53dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="120dp"
            android:layout_marginTop="136dp"
            android:layout_marginEnd="114dp"
            android:layout_marginBottom="200dp"
            android:background="@drawable/buttonst"
            android:fontFamily="serif-monospace"
            android:gravity="center"
            android:onClick="Korsina"
            android:rotation="0"
            android:rotationX="0"
            android:rotationY="0"
            android:text="Корзина"
            android:textAllCaps="false"
            android:textSize="18sp" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout4"
        android:layout_width="412dp"
        android:layout_height="62dp"
        android:background="@color/purple_500"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <Button
            android:id="@+id/button20"
            android:layout_width="wrap_content"
            android:layout_height="49dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="105dp"
            android:layout_marginTop="9dp"
            android:layout_marginEnd="105dp"
            android:layout_marginBottom="4dp"
            android:background="@drawable/middlebtn"
            android:onClick="BackStepBtn"
            android:text="Назад"
            android:textAllCaps="false"
            android:textSize="18sp" />

    </RelativeLayout>

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="138dp"
        android:layout_height="119dp"
        android:rotation="-6"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.059"
        tools:srcCompat="@tools:sample/avatars" />

</androidx.constraintlayout.widget.ConstraintLayout>